###H3K9ME3 & H4K20ME3 ANALYSIS

#H3K9me3
computeMatrix scale-regions -a 500 -b 500 --regionBodyLength 2500 \
 -S $P149DIR/149-104_ChIP_WT_H3K9me3_Rep3_S128.bin_25.smooth_75Bulk.bw \
    $P149DIR/149-105_ChIP_cac-1_H3K9me3_Rep3_S129.bin_25.smooth_75Bulk.bw \
    $P149DIR/149-106_ChIP_cac-2_H3K9me3_Rep3_S130.bin_25.smooth_75Bulk.bw \
    $P149DIR/149-108_ChIP_set-7_Input_Rep3_S132.bin_25.smooth_75Bulk.bw \
 -R $BEDDIR2/WT_H3K9me3_Rep1_peaks.bed --skipZeros --missingDataAsZero -o K9_matrix_PRC2.gz

    plotHeatmap -m K9_matrix_PRC2.gz -out CAF-1_K9_regions.pdf \
                    --samplesLabel WT cac-1 cac-2 set-7 \
                    --startLabel "5'" \
                    --endLabel "3'" \
                    --sortRegions descend \
                    --sortUsingSamples 1 \
                    --missingDataColor 1 \
                    --heatmapHeight 15 \
                    --colorList 'white, mediumblue' \
                    --zMin 0 \

#H4K20me3
computeMatrix scale-regions -a 500 -b 500 --regionBodyLength 2500 \
 -S $P133DIR/133-19_ChIP_WT_H4K20me3_Rep1_S17.bin_25.smooth_75Bulk.bw \
    $P133DIR/133-20_ChIP_cac-1_H4K20me3_Rep1_S18.bin_25.smooth_75Bulk.bw \
