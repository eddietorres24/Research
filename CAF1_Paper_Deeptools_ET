    -R $BEDDIR2/K27genes_K36enriched.bed --skipZeros -o H4K20_matrix_PRC2.gz

    plotHeatmap -m H4K20_matrix_PRC2.gz -out CAF-1_H4K20me3_K27_genes_K36enriched.pdf \
