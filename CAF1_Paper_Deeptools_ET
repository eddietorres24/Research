    -R $BEDDIR2/WT_H4K20me3_peaks.bed --skipZeros -o H4K20_matrix_PRC2.gz

    plotHeatmap -m H4K20_matrix_PRC2.gz -out CAF-1_H4K20me3_allpeaks.pdf \
                    --samplesLabel WT cac-1 cac-2 set-7 \
                    --startLabel "5'" \
                    --endLabel "3'" \
                    --sortRegions descend \
                    --sortUsingSamples 1 \
                    --missingDataColor 1 \
                    --colorList 'white, blueviolet' \
                    --zMin 0 \

